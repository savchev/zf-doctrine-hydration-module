{
  "name": "phpro/zf-doctrine-hydration-module",
  "license": "MIT",
  "description": "Doctrine hydrators for ZF2 and ZF3",
  "keywords": [
    "zf",
    "doctrine",
    "hydrator"
  ],
  "authors": [
    {
      "name": "Toon Verwerft",
      "email": "toon.verwerft@phpro.be"
    }
  ],
  "extra": {
    "zf": {
      "module": "Phpro\\DoctrineHydrationModule"
    }
  },
  "require": {
    "php": "^7.3",
    "doctrine/doctrine-module": "^4.0",
    "doctrine/instantiator": "^1.3",
    "laminas/laminas-servicemanager": "^3.4",
    "alcaeus/mongo-php-adapter": "^1.1",
    "doctrine/doctrine-laminas-hydrator": "^2.0",
    "mongodb/mongodb": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "doctrine/doctrine-orm-module": "^3.0",
    "phpro/grumphp": "^0.18.0",
    "squizlabs/php_codesniffer": "^3.5",
    "doctrine/doctrine-mongo-odm-module": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Phpro\\DoctrineHydrationModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhproTest\\DoctrineHydrationModule\\": "test/src/"
    }
  },
  "config": {
    "platform": {
      "ext-mongo": "1.6.16"
    }
  }
}
